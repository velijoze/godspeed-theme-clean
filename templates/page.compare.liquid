{% comment %}
  Template: Compare Page with Bike Comparison Tool
  This template displays the bike comparison functionality
{% endcomment %}

<div class="page-width">
  <header class="section-header">
    <h1 class="section-header__title">{{ page.title }}</h1>
  </header>

  {% if page.content != blank %}
    <div class="rte">
      {{ page.content }}
    </div>
  {% endif %}

  {% section 'bike-compare' %}
</div>